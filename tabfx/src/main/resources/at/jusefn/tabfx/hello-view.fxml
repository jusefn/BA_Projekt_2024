                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                                 <children>
                                    <TextField fx:id="calcField" editable="false" layoutX="11.0" layoutY="14.0" prefHeight="25.0" prefWidth="138.0" />
                                 </children>
                              </AnchorPane>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="257.0" prefWidth="184.0">
                                 <children>
                                    <Button layoutX="10.0" layoutY="58.0" mnemonicParsing="false" onAction="#onPlusClick" text="+" />
                                    <Button layoutX="10.0" layoutY="97.0" mnemonicParsing="false" onAction="#onMinusClick" prefHeight="25.0" prefWidth="23.0" text="-" />
                                    <Button layoutX="11.0" layoutY="135.0" mnemonicParsing="false" onAction="#onMultiplyClick" prefHeight="25.0" prefWidth="24.0" text="*" />
                                    <Button layoutX="10.0" layoutY="167.0" mnemonicParsing="false" onAction="#onDivideClick" prefHeight="25.0" prefWidth="25.0" text="/" />
                                    <Button layoutX="78.0" layoutY="58.0" mnemonicParsing="false" onAction="#onEightClick" text="8" />
                                    <Button layoutX="46.0" layoutY="135.0" mnemonicParsing="false" onAction="#onOneClick" text="1" />
                                    <Button layoutX="78.0" layoutY="135.0" mnemonicParsing="false" onAction="#onTwoClick" text="2" />
                                    <Button layoutX="109.0" layoutY="135.0" mnemonicParsing="false" onAction="#onThreeClick" text="3" />
                                    <Button layoutX="78.0" layoutY="167.0" mnemonicParsing="false" onAction="#onZeroClick" text="0" />
