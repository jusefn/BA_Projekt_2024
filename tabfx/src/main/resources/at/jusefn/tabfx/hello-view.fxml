                              <ToolBar prefHeight="41.0" prefWidth="338.0">
                                 <items>
                                    <Button mnemonicParsing="false" onAction="#onNewButtonClick" text="New" />
                                  <Button mnemonicParsing="false" onAction="#onLoadButtonClick" text="Load" />
                                    <Button mnemonicParsing="false" onAction="#onSaveButtonClick" text="Save" />
                                 </items>
                              </ToolBar>
                              <TextArea fx:id="editField" prefHeight="266.0" prefWidth="338.0" />
                           </children>
                        </VBox>
                     </children></AnchorPane>
            </content>
          </Tab>
